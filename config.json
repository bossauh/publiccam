{
  "bruteforcer": {
    "output_paths": [
      "/out.jpg",
      "/video.mjpg",
      "/cam_{index}.cgi",
      "/video",
      "/cgi-bin/camera",
      "/mjpg/video.mjpg",
      "/cgi-bin/faststream.jpg",
      "/webcapture.jpg"
    ],
    "port_range": [8000, 8010],
    "content_types": ["image/jpeg", "multipart/x-mixed-replace"],
    "batch_size": 1000
  },
  "database": {
    "default_db_name": "publiccam",
    "host": "localhost:27017"
  },
  "aiohttp": {
    "timeout": {
      "total": null,
      "connect": 60,
      "sock_connect": 60,
      "sock_read": 10
    }
  }
}
